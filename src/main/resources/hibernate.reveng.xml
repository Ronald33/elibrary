  <table-filter match-name="libro_autor"/>
  <table-filter match-name="comentario"/>
  <table-filter match-name="editor"/>
  <table-filter match-name="autor"/>
  <table-filter match-name="categoria"/>
  <table-filter match-name="editorial"/>
  <table-filter match-name="puntuacion"/>
</hibernate-reverse-engineering>
